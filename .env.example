# IF USING AN EXTERNAL DATABASE
DATABASE_URL="postgresql://user:password@localhost:PORT/db-name"

# if using the docker compose
# sudo docker compose up dev-db -d : to mount the docker compose
POSTGRES_USER="postgresql_user"
